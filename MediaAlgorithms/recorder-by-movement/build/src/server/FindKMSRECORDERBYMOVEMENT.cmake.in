  list (APPEND KMSRECORDERBYMOVEMENT_INCLUDE_DIRS ${KMSRECORDERBYMOVEMENT_IMPLEMENTATION_GENERATED_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSRECORDERBYMOVEMENT_IMPLEMENTATION_GENERATED_INCLUDE_DIR)
