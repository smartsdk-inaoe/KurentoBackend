if (NOT "@_PARAM_INTERFACE_LIB_EXTRA_HEADERS@" STREQUAL "")
  if (TARGET kmsrecorderbymovementinterface)
    set (KMSRECORDERBYMOVEMENT_INTERFACE_EXTRA_INCLUDE_DIR "${KMSRECORDERBYMOVEMENT_SOURCE_DIR_PREFIX}/@_PARAM_INTERFACE_LIB_EXTRA_HEADERS_PREFIX@")
  else ()
    find_path(KMSRECORDERBYMOVEMENT_INTERFACE_EXTRA_INCLUDE_DIR
